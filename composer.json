{
  "name": "bloodlog/isolated-architecture",
  "description": "",
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "bloodlog",
      "email": "bloodlog@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bloodlog\\Src\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6.9",
    "mockery/mockery": "^1.6"
  },
  "prefer-stable": true,
  "scripts": {
    "test": [
      "phpunit tests/*.php"
    ]
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
